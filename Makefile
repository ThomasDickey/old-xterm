resize: $(OBJS2)
	rm -f $@
	$(CC) -o $@ $(OBJS2)  $(LDFLAGS) -lc -ltermcap

relink::
	rm -f resize
	$(MAKE) $(MFLAGS) resize

clean::
	rm -f resize

install:: resize
	$(INSTALL) -c resize $(BINDIR)

clean::
	$(RM_CMD) \#*

